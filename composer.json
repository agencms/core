{
    "name": "agencms/core",
    "description": "Laravel CMS Library for Agencms",
    "type": "library",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Marco Mark",
            "email": "marco@silvanite.com"
        }
    ],
    "require": {
        "barryvdh/laravel-cors": "^0.11",
        "laravel/framework": "^5.4|^5.5|^5.6",
        "agencms/auth": "^1.0",
        "thesold/laravel-media-manager": "~1.1",
        "spatie/laravel-view-components": "^1.1",
        "agencms/settings": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Agencms\\Core\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Agencms\\Core\\Providers\\AgencmsServiceProvider"
            ],
            "aliases": {
                "Agencms": "Agencms\\Core\\Facades\\Agencms",
                "RenderEngine": "Agencms\\Core\\Facades\\RenderEngine"
            }
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.1"
    }
}
