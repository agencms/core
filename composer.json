{
    "name": "silvanite/agencms",
    "description": "Laravel CMS Library for Agencms",
    "type": "library",
    "authors": [
        {
            "name": "Marco Mark",
            "email": "marco@silvanite.com"
        }
    ],
    "require": {
        "barryvdh/laravel-cors": "^0.11.0",
        "laravel/framework": "^5.4|^5.5|^5.6",
        "silvanite/agencms-auth": "dev-master",
        "thesold/laravel-media-manager": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Silvanite\\Agencms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Silvanite\\Agencms\\Providers\\AgencmsServiceProvider"
            ],
            "aliases": {
                "Agencms": "Silvanite\\Agencms\\Facades\\Agencms",
                "RenderEngine": "Silvanite\\Agencms\\Facades\\RenderEngine"
            }
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.1"
    }
}
